// 将设置放入此文件中以覆盖默认值和用户设置。
{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "**/.vscode/chrome": true,
        "**/*.meta": true,
        "**/temp": true,
        "**/local": true,
        "**/build": true,
        "**/library": true
    },

    "files.watcherExclude": {
        "**/.git/objects": true,
        "**/node_modules": true,
        "**/temp": true
    },

    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/temp": true,
        "**/local": true,
        "**/build": true,
        "**/library": true
    }
}